{
  "name": "Redux-Server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/server/server.js",
    "clean": "rm -rf build && mkdir build",
    "devServer": "webpack-dev-server --hot --progress --host 0.0.0.0 --port 8000 --env.base=client",
    "dev": "DEBUG=express:* npm run build && npm start",
    "babel": "babel-node ./src/server/server.js",
    "nodemon": "nodemon -w src --exec \"npm run dev\"",
    "nodemon:fast": "nodemon -w src --exec \"npm run babel\"",
    "build": "npm run build:client && npm run build:server",
    "build:prod": "NODE_ENV=production npm run build",
    "build:server": "webpack --env.base=server",
    "build:client": "webpack --env.base=client",
    "postinstall": "npm run build"
  },
