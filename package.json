{
  "name": "Single-Page-SSR",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/server/server.js",
    "clean": "rm -rf build && mkdir build",
    "devServer": "webpack-dev-server --hot --progress --host 0.0.0.0 --port 8000 --env.base=client",
    "dev": "DEBUG=express:* npm run build && npm start",
    "babel": "babel-node ./src/server/server.js",
    "nodemon": "nodemon -w src --exec \"npm run dev\"",
    "nodemon:fast": "nodemon -w src --exec \"npm run babel\"",
    "build": "npm run build:client && npm run build:server",
    "build:prod": "NODE_ENV=production npm run build",
    "build:server": "webpack --env.base=server",
    "build:client": "webpack --env.base=client",
    "postinstall": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "helmet": "^3.11.0",
    "history": "^4.7.2",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "normalize.css": "^8.0.0",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "reducer-builder": "^1.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-react-displayname": "0.0.4",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.10",
    "html-webpack-plugin": "^2.30.1",
    "postcss": "^6.0.19",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
