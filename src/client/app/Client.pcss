* {
    -webkit-margin-before:0;
    -webkit-margin-after:0;
    -webkit-margin-start:0;
    -webkit-margin-end:0
}

body {
    margin:0
}

.fill {
    width: 100%;
    height: 100%;
}

.client {
    display: flex;
    flex-direction: column;

    img {
        max-width:100%;
        height:auto;
        display:inline-block;
        vertical-align:middle
    }
}
